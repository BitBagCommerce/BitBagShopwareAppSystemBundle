           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service class="BitBag\ShopwareAppSystemBundle\EventListener\RequestListener"
                 id="bitbag.shopware_app_system.event_listener.request_listener">
            <tag name="kernel.event_listener" event="kernel.request" priority="1000"/>
        </service>
    </services>
</container>
